<Window x:Class="CommonErrorsKata.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommonErrorsKata"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ProgressBar x:Name="progress" HorizontalAlignment="Left" Height="26" Margin="10,10,0,0" VerticalAlignment="Top" Width="600"/>
        <Label x:Name="percentLabel" Content="Label" HorizontalAlignment="Left" Margin="615,10,0,0" VerticalAlignment="Top" Width="167"/>
        <Image x:Name="imageBox" HorizontalAlignment="Left" Height="368" Margin="10,41,0,0" VerticalAlignment="Top" Width="600"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="368" Margin="615,41,0,0" VerticalAlignment="Top" Width="167" SelectionChanged="ListBox_SelectionChanged"/>

    </Grid>
</Window>
